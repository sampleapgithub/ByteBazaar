{
    "version" : 2,
    "name" : "ecommerce",
    "builds" : [
        {
            "src" : "./api/index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src" : "/api/(.*)",
            "dest" : "/api/index.js"
        },
        {
            "src" : "/(.*)",
            "dest" : "/api/index.js"
        }
    ]
}
